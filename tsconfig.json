{
  "compilerOptions": {
    "outDir": "./dist/",
    "rootDir": "./",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "checkJs": false,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "strict": true,
    "alwaysStrict": true,
    "strictNullChecks": true
  },
  "include":[
    "src"
  ]
}